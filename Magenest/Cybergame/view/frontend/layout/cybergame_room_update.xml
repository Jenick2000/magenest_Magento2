<?xml version="1.0"?>

<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <update handle="customer_account"/>
    <body>
        <referenceContainer name="content">
            <block class="Magenest\Cybergame\Block\RoomOption\ListRoom"
                   name="room_info_list"
                   template="Magenest_Cybergame::room/list.phtml" cacheable="false"/>
        </referenceContainer>
        <referenceBlock name="root">
            <action method="setHeaderTitle">
                <argument translate="true" name="title" xsi:type="string">Update Room Info</argument>
            </action>
        </referenceBlock>
    </body>
</page>
